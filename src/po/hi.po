msgstr "'%s' के लिए कूटशब्द दाखिल करें."

#: ../libseahorse/seahorse-passphrase.c:360
msgid "Enter new passphrase"
msgstr "नया कूटशब्द दाखिल करें"

#: ../libseahorse/seahorse-passphrase.c:362
msgid "Enter passphrase"
msgstr "कूटशब्द दाखिल करें"

#: ../libseahorse/seahorse-prefs.c:100
msgid "None. Prompt for a key."
msgstr "कोई नहीं. किसी कुंजी के लिए प्रांप्ट."

#: ../libseahorse/seahorse-prefs.glade.h:1
msgid "<b>Default Key</b>"
msgstr "<b>तयशुदा कुंजी</b>"

#: ../libseahorse/seahorse-prefs.glade.h:2
msgid "<b>Remember PGP Passphrases</b>"
msgstr "<b>PGP कूटशब्द याद रखें</b>"

#: ../libseahorse/seahorse-prefs.glade.h:3
msgid "<i>A supported PGP passphrase caching agent is not running.</i>"
msgstr "<i>एक समर्थित PGP कूटशब्द कैशिंग प्रतिनिधि चल नहीं रहा है.</i>"

#: ../libseahorse/seahorse-prefs.glade.h:4
msgid "<i>This key is used to sign messages when no other key is chosen</i>"
msgstr "<i>यह कुंजी संदेश को हस्ताक्षरित करने के लिए प्रयोग किया जाता है जब कोई दूसरी कुंजी नहीं चुनी गई है</i>"


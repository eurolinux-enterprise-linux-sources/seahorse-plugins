msgid "Passphrase"
msgstr "कूटशब्द वाक्यांश"

#: ../libseahorse/seahorse-passphrase.c:203
msgid "Password:"
msgstr "पासवर्ड: "

#: ../libseahorse/seahorse-passphrase.c:266
msgid "Confirm:"

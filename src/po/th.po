#: ../libseahorse/seahorse-notification.c:646
msgid "Bad or forged signature. The signed data was modified."
msgstr "ลายเซ็นเสียหรือถูกปลอมแปลง ข้อมูลที่เซ็นไว้มีการเปลี่ยนแปลง"

#: ../libseahorse/seahorse-notification.c:647
msgid "Bad Signature"
msgstr "ลายเซ็นเสีย"

#: ../libseahorse/seahorse-notification.c:655
msgid "Couldn't verify signature."
msgstr "ไม่สามารถตรวจสอบยืนยันลายเซ็น"

#: ../libseahorse/seahorse-notify.glade.h:1
msgid "Notification Messages"
msgstr "ข้อความแจ้ง"

#: ../libseahorse/seahorse-passphrase.c:174
msgid "Passphrase"
msgstr "วลีรหัสผ่าน"

#: ../libseahorse/seahorse-passphrase.c:177
msgid "Password:"
msgstr "รหัสผ่าน:"

#: ../libseahorse/seahorse-passphrase.c:241
msgid "Confirm:"
msgstr "ยืนยัน:"

#: ../libseahorse/seahorse-passphrase.c:324
#, c-format
msgid "Wrong passphrase."
msgstr "วลีรหัสผ่านไม่ถูกต้อง"

#: ../libseahorse/seahorse-passphrase.c:328
#, c-format
msgid "Enter new passphrase for '%s'"
msgstr "กรุณาตั้งวลีรหัสผ่านใหม่สำหรับ '%s'"

#: ../libseahorse/seahorse-passphrase.c:330
#, c-format
msgid "Enter passphrase for '%s'"
msgstr "กรุณาป้อนวลีรหัสผ่านสำหรับ '%s'"

#: ../libseahorse/seahorse-passphrase.c:333
msgid "Enter new passphrase"
msgstr "กรุณาตั้งวลีรหัสผ่านใหม่"

#: ../libseahorse/seahorse-passphrase.c:335
msgid "Enter passphrase"
msgstr "กรุณาป้อนวลีรหัสผ่าน"

#: ../libseahorse/seahorse-prefs.c:100
msgid "None. Prompt for a key."
msgstr "ไม่มี ให้ถามหากุญแจเอา"

#: ../libseahorse/seahorse-prefs.glade.h:1
msgid "<b>Default Key</b>"
msgstr "<b>กุญแจปริยาย</b>"

#: ../libseahorse/seahorse-prefs.glade.h:2
msgid "<b>Remember PGP Passphrases</b>"
msgstr "<b>จำวลีรหัสผ่าน PGP</b>"

#: ../libseahorse/seahorse-prefs.glade.h:3
msgid "<i>A supported PGP passphrase caching agent is not running.</i>"
msgstr "<i>ไม่มีเอเจนต์สำหรับแคชวลีรหัสผ่าน PGP ที่รองรับทำงานอยู่</i>"

#: ../libseahorse/seahorse-prefs.glade.h:4
msgid "<i>This key is used to sign messages when no other key is chosen</i>"
